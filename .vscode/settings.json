{
  "cSpell.words": ["dotenv", "jsonify", "pymssql", "werkzeug"],
  "files.associations": { ".djlintrc": "json", "*.html": "html" },
  "[python]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "[html]": {
    "editor.tabSize": 2
  },
  "[jinja-html]": {
    "editor.tabSize": 2
  },
  "tailwindCSS.includeLanguages": {
    "jinja-html": "html"
  },
  "python.formatting.provider": "none"
}
